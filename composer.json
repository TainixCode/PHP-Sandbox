{
    "name": "tainix/tainix-php",
    "description": "Sandbox PHP pour rÃ©aliser les challenges Tainix",
    "require": {
        "php": ">=7.4",
        "pestphp/pest": "1.*",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.4",
        "illuminate/collections": "^10.43"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4"
    },
    "suggest": {
        "illuminate/collections": "*"
    },
    "authors": [
        {
            "name": "Arthur Weill pour Tainix",
            "email": "bonjour@tainix.fr"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Challenges\\" : "challenges/",
            "Tainix\\" : "tainix/",
            "Pest\\" : "pest/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
